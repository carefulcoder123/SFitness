{
  "name": "sfitness",
  "version": "1.0.0",
  "description": "SFitness responsive website built using HTML5, CSS3, JavaScript. Grid, Flexbox, animations used for the layout of page.",
  "scripts": {
    "dev": "parcel index.html",
    "build": "parcel build $(pwd)/index.html",
    "watch:sass": "sass --watch sass/main.scss:css/main.css",
    "prefix:css": "postcss css/main.css --use autoprefixer -o css/main.css"
  },
  "author": "Sankalp",
  "license": "ISC",
  "devDependencies": {
    "parcel": "^2.10.0",
    "sass": "^1.74.1"
  },
  "dependencies": {
    "aos": "^3.0.0-beta.6",
    "leaflet": "^1.9.4"
  }
}
